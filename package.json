{
  "name": "@skygear/skycli",
  "version": "2.0.0-alpha.5",
  "description": "Skycli is tool for managing Skygear cluster",
  "main": "index.js",
  "bin": {
    "skycli": "./bin/skycli.js"
  },
  "scripts": {
    "test": "jest",
    "typecheck": "tsc --noEmit",
    "lint": "eslint 'src/**/*.ts'",
    "prettier": "prettier --write 'src/**/*.ts'",
    "prettier-check": "prettier --check 'src/**/*.ts'",
    "prepare": "gulp build",
    "start": "gulp watch",
    "skycli": "gulp build && ./bin/skycli.js",
    "build-bin": "gulp build && mkdir -p dist-bin && pkg --out-path dist-bin --targets node8-macos,node8-linux,node8-win .",
    "clean": "rm -rf dist dist-bin",
    "prepare-new-release": "git log --first-parent `git describe --abbrev=0`.. > new-release"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SkygearIO/skycli.git"
  },
  "keywords": [
    "cli",
    "skygear"
  ],
  "author": "Oursky Limited",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/SkygearIO/skycli/issues"
  },
  "homepage": "https://skygear.io/",
  "dependencies": {
    "@babel/polyfill": "^7.4.4",
    "@skygear/node-client": "2.0.0-alpha.16",
    "@zeit/dockerignore": "0.0.5",
    "chalk": "^2.4.2",
    "cli-table3": "^0.5.1",
    "external-editor": "^3.1.0",
    "form-data": "^2.5.1",
    "fs-extra": "^4.0.3",
    "gunzip-maybe": "^1.4.1",
    "inquirer": "^7.0.0",
    "js-yaml": "^3.13.1",
    "lodash": "^4.17.15",
    "mime": "2.4.4",
    "moment": "^2.24.0",
    "node-fetch": "^1.7.3",
    "tar-fs": "^2.0.0",
    "untildify": "^3.0.3",
    "yargs": "^13.2.2"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-typescript": "^7.3.3",
    "@babel/register": "^7.5.5",
    "@oursky/eslint-config": "0.18.0",
    "@types/core-js": "^0.9.46",
    "@types/fs-extra": "^5.0.5",
    "@types/inquirer": "6.5.0",
    "@types/jest": "^24.0.13",
    "@types/js-yaml": "^3.12.1",
    "@types/lodash": "^4.14.92",
    "@types/mime": "2.0.1",
    "@types/moment": "^2.13.0",
    "@types/node": "^10.0.0",
    "@types/node-fetch": "^2.3.3",
    "@types/query-string": "^5.1.0",
    "@types/untildify": "^3.0.0",
    "@types/yargs": "^13.0.0",
    "@typescript-eslint/eslint-plugin": "1.9.0",
    "@typescript-eslint/parser": "1.9.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "eslint": "5.16.0",
    "eslint-config-prettier": "4.3.0",
    "eslint-plugin-react": "7.13.0",
    "eslint-plugin-react-hooks": "1.6.0",
    "eslint-plugin-react-native": "3.7.0",
    "extend": "^3.0.2",
    "gulp": "^4.0.0",
    "gulp-babel": "^8.0.0",
    "gulp-exclude-gitignore": "^1.2.0",
    "jest": "^24.8.0",
    "pkg": "^4.3.7",
    "prettier": "^1.16.4",
    "require-dir": "^0.3.2",
    "typescript": "3.3.3333"
  },
  "resolutions": {
    "natives": "1.1.3"
  },
  "jest": {
    "setupFiles": [
      "<rootDir>/src/setupJest.js"
    ]
  },
  "pkg": {
    "assets": [
      "dist/**/*",
      "templates/**/*"
    ]
  },
  "skycli": {}
}
