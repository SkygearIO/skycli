    "@babel/plugin-transform-typeof-symbol": "7.7.4",
    "@babel/plugin-transform-typescript": "7.7.4",
    "@babel/plugin-transform-unicode-regex": "7.7.4",
    "@babel/runtime": "7.7.7",
    "@oursky/eslint-config": "1.3.1",
