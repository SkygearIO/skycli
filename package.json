{
  "name": "@skygear/skycli",
  "version": "2.0.0-alpha.7",
  "description": "Skycli is tool for managing Skygear cluster",
  "main": "dist/skycli.js",
  "bin": {
    "skycli": "./dist/skycli.js"
  },
  "scripts": {
    "prepare": "rollup --config",
    "skycli": "rollup --config && ./dist/skycli.js",
    "build-bin": "npm run prepare && mkdir -p dist-bin && pkg --out-path dist-bin --targets node10-macos,node10-linux,node10-win .",
    "test": "jest",
    "typecheck": "tsc --noEmit",
    "lint": "eslint 'src/**/*.ts'",
    "prettier": "prettier --write 'src/**/*.ts'",
    "prettier-check": "prettier --check 'src/**/*.ts'",
    "clean": "rm -rf dist dist-bin",
    "prepare-new-release": "git log --first-parent `git describe --abbrev=0`.. > new-release"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SkygearIO/skycli.git"
  },
  "keywords": [
    "cli",
    "skygear"
  ],
  "author": "Oursky Limited",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/SkygearIO/skycli/issues"
  },
  "homepage": "https://skygear.io/",
  "dependencies": {
    "@skygear/node-client": "2.0.0-alpha.16",
    "@zeit/dockerignore": "0.0.5",
    "chalk": "2.4.2",
    "cli-table3": "0.5.1",
    "core-js": "3.6.4",
    "external-editor": "3.1.0",
    "fs-extra": "4.0.3",
    "gunzip-maybe": "1.4.1",
    "inquirer": "7.0.2",
    "js-yaml": "3.13.1",
    "lodash": "4.17.15",
    "mime": "2.4.4",
    "moment": "2.24.0",
    "node-fetch": "1.7.3",
    "tar-fs": "2.0.0",
    "untildify": "3.0.3",
    "yargs": "13.3.0"
  },
  "devDependencies": {
    "@babel/cli": "7.8.3",
    "@babel/core": "7.8.3",
    "@babel/plugin-proposal-class-properties": "7.8.3",
    "@babel/plugin-transform-typescript": "7.8.3",
    "@babel/preset-env": "7.8.3",
    "@oursky/eslint-config": "1.3.1",
    "@rollup/plugin-commonjs": "11.0.1",
    "@rollup/plugin-json": "4.0.1",
    "@rollup/plugin-node-resolve": "6.1.0",
    "@types/fs-extra": "5.1.0",
    "@types/inquirer": "6.5.0",
    "@types/jest": "24.0.25",
    "@types/js-yaml": "3.12.1",
    "@types/lodash": "4.14.149",
    "@types/mime": "2.0.1",
    "@types/moment": "2.13.0",
    "@types/node": "10.17.13",
    "@types/node-fetch": "2.5.4",
    "@types/query-string": "5.1.0",
    "@types/untildify": "3.0.0",
    "@types/yargs": "13.0.4",
    "builtins": "3.0.0",
    "eslint": "6.8.0",
    "jest": "24.9.0",
    "pkg": "4.4.2",
    "prettier": "1.19.1",
    "rollup": "1.28.0",
    "rollup-plugin-babel": "4.3.3",
    "typescript": "3.7.4"
  },
  "jest": {
    "rootDir": "src"
  },
  "pkg": {
    "assets": [
      "dist/**/*"
    ]
  },
  "skycli": {}
}
